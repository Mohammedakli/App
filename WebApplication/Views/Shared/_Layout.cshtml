    @Scripts.Render("~/userCtrl")
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/angular-route.min.js"></script>


    <script src="~/Web/app.js"></script>
    <script src="~/Web/controllers/userCtrl.js"></script>
    <script src="~/Web/shared/services/dataService.js"></script>

    <a href="~/Views/User/Index.cshtml">~/Views/User/UserList.cshtml</a>



    @RenderSection("scripts", required: false)
